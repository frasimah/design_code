# ============================================
# Design Code Panel - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values

# ===================
# AI Configuration 1
# ===================
GEMINI_API_KEY=your_google_gemini_api_key_here_this

# Proxy settings (optional, for restricted networks)
# GEMINI_PROXY_URL=socks5://user:pass@host:port

# ===================
# WooCommerce Integration (Optional)
# ===================
WC_CONSUMER_KEY=
WC_CONSUMER_SECRET=
WC_BASE_URL=https://your-site.ru/wp-json/wc/v3

# ===================
# Telegram Bot (Optional)
# ===================
TELEGRAM_BOT_TOKEN=

# ===================
# SSL Configuration
# ===================
HTTPX_VERIFY_SSL=true

# ===================
# Rate Limiting (Optional)
# ===================
# REQUESTS_PER_SECOND=2
# DELAY_BETWEEN_REQUESTS=0.5

# ===================
# NextAuth.js Authentication
# ===================
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-random-secret-key-here-at-least-32-chars

# ===================
# API Configuration
# ===================
NEXT_PUBLIC_API_URL=https://domain
BACKEND_PORT=8001
FRONTEND_PORT=3001

# ===================
# OAuth Providers (at least one required for production)
# ===================
# Yandex OAuth - https://oauth.yandex.ru/
YANDEX_CLIENT_ID=
YANDEX_CLIENT_SECRET=

# Google OAuth - https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
